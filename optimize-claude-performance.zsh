# üîß Auto-tune system for optimal performance! üéõÔ∏è
claude-perf-tune() {
    echo "üîß Claude Performance Auto-Tuner"
    echo "‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê"
    echo "üéØ Optimizing your system for maximum Claude Code performance!"
    echo ""
    
    # üìä System analysis
    local cpu_cores=$(nproc 2>/dev/null || sysctl -n hw.ncpu 2>/dev/null || echo "4")
    local ram_gb=$(free -g 2>/dev/null | awk '/^Mem:/{print $2}' || sysctl -n hw.memsize 2>/dev/null | awk '{print int($1/1073741824)}' || echo "8")
    local load_avg=$(uptime | awk -F'load average:' '{print $2}' | awk '{print $1}' | tr -d ',' || echo "0.0")
    
    echo "üìä System Analysis:"
    echo "  üß† CPU Cores: $cpu_cores"
    echo "  üíæ RAM: ${ram_gb}GB"
    echo "  üìà Load Average: $load_avg"
    echo ""
    
    # üéØ Performance recommendations
    echo "üéØ Performance Recommendations:"
    
    # üöÄ Parallel job optimization
    local optimal_parallel=$((cpu_cores / 2))
    [[ $optimal_parallel -lt 2 ]] && optimal_parallel=2
    [[ $optimal_parallel -gt 8 ]] && optimal_parallel=8
    
    echo "  ‚ö° Optimal parallel jobs: $optimal_parallel (based on $cpu_cores cores)"
    export CLAUDE_PARALLEL_JOBS="$optimal_parallel"
    
    # üì¶ Batch size optimization
    local optimal_batch=$((ram_gb / 2))
    [[ $optimal_batch -lt 3 ]] && optimal_batch=3
    [[ $optimal_batch -gt 15 ]] && optimal_batch=15
    
    echo "  üì¶ Optimal batch size: $optimal_batch (based on ${ram_gb}GB RAM)"
    export CLAUDE_BATCH_SIZE="$optimal_batch"
    
    # ‚è∞ TTL optimization based on usage
    if [[ "${CLAUDE_REDIS_AVAILABLE}" == "true" ]]; then
        local key_count=$(redis-cli dbsize 2>/dev/null || echo "0")
        if [[ $key_count -gt 1000 ]]; then
            export CLAUDE_CACHE_TTL="3600"  # 1 hour for high usage
            echo "  üíæ Cache TTL: 3600s (high usage detected: $key_count keys)"
        else
            export CLAUDE_CACHE_TTL="7200"  # 2 hours for low usage
            echo "  üíæ Cache TTL: 7200s (normal usage: $key_count keys)"
        fi
    fi
    
    # üìÅ Output directory optimization
    mkdir -p "${CLAUDE_OUTPUT_DIR}"
    chmod 755 "${CLAUDE_OUTPUT_DIR}" 2>/dev/null
    echo "  üìÅ Output directory: ${CLAUDE_OUTPUT_DIR} (created and verified)"
    
    echo ""
    echo "‚úÖ Auto-tuning completed! New settings:"
    echo "  ‚ö° Parallel Jobs: $CLAUDE_PARALLEL_JOBS"
    echo "  üì¶ Batch Size: $CLAUDE_BATCH_SIZE"
    echo "  üíæ Cache TTL: ${CLAUDE_CACHE_TTL}s"
    echo ""
    echo "üí° Run 'claude-perf-status' to see the full optimization report!"
}

# üìà Real-time performance monitor! üìä
claude-perf-monitor() {
    local duration="${1:-60}"
    
    echo "üìà Claude Performance Monitor"
    echo "‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê"
    echo "‚è±Ô∏è Monitoring for $duration seconds..."
    echo "üéØ Press Ctrl+C to stop early"
    echo ""
    
    local start_time=$(date +%s)
    local sample_count=0
    
    while [[ $(($(date +%s) - start_time)) -lt $duration ]]; do
        ((sample_count++))
        local current_time=$(date +%H:%M:%S)
        
        echo "üìä Sample #$sample_count at $current_time:"
        
        # üíæ Memory usage
        local mem_usage=$(free 2>/dev/null | awk '/^Mem:/{printf "%.1f", $3/$2*100}' || echo "N/A")
        echo "  üíæ Memory: ${mem_usage}%"
        
        # ‚ö° Load average
        local load=$(uptime | awk -F'load average:' '{print $2}' | awk '{print $1}' | tr -d ',' || echo "N/A")
        echo "  ‚ö° Load: $load"
        
        # üåê Active Claude jobs (if async available)
        if [[ "${CLAUDE_ASYNC_AVAILABLE}" == "true" ]]; then
            local active_jobs=$(jobs | wc -l)
            echo "  üöÄ Active Jobs: $active_jobs"
        fi
        
        # üíæ Redis stats (if available)
        if [[ "${CLAUDE_REDIS_AVAILABLE}" == "true" ]]; then
            local redis_keys=$(redis-cli dbsize 2>/dev/null || echo "N/A")
            local redis_mem=$(redis-cli info memory 2>/dev/null | grep used_memory_human | cut -d: -f2 | tr -d '\r' || echo "N/A")
            echo "  üîë Redis Keys: $redis_keys"
            echo "  üíæ Redis Memory: $redis_mem"
        fi
        
        echo ""
        sleep 5
    done
    
    echo "‚úÖ Monitoring completed after $duration seconds!"
    echo "üìä Total samples: $sample_count"
}

