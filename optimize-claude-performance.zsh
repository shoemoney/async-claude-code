# üîß Performance configuration setup! ‚öôÔ∏è
claude_performance_config() {
    # üéØ Optimal performance settings based on system resources
    local cpu_cores=$(nproc 2>/dev/null || sysctl -n hw.ncpu 2>/dev/null || echo "4")
    local ram_gb=$(free -g 2>/dev/null | awk '/^Mem:/{print $2}' || sysctl -n hw.memsize 2>/dev/null | awk '{print int($1/1073741824)}' || echo "8")
    
    # üöÄ Dynamic performance optimization based on system specs
    if [[ $cpu_cores -ge 8 && $ram_gb -ge 16 ]]; then
        # üí™ High-performance system
        export CLAUDE_BATCH_SIZE="10"
        export CLAUDE_PARALLEL_JOBS="6"
        export CLAUDE_CACHE_TTL="7200"
        export CLAUDE_PERFORMANCE_MODE="TURBO"
        echo "üí™ TURBO MODE: High-performance system detected!"
    elif [[ $cpu_cores -ge 4 && $ram_gb -ge 8 ]]; then
        # ‚ö° Standard performance system
        export CLAUDE_BATCH_SIZE="5"
        export CLAUDE_PARALLEL_JOBS="3"
        export CLAUDE_CACHE_TTL="3600"
        export CLAUDE_PERFORMANCE_MODE="STANDARD"
        echo "‚ö° STANDARD MODE: Balanced performance configuration"
    else
        # üõ°Ô∏è Conservative mode for limited resources
        export CLAUDE_BATCH_SIZE="3"
        export CLAUDE_PARALLEL_JOBS="2"
        export CLAUDE_CACHE_TTL="1800"
        export CLAUDE_PERFORMANCE_MODE="CONSERVATIVE"
        echo "üõ°Ô∏è CONSERVATIVE MODE: Resource-optimized configuration"
    fi
    
    # üåê Universal performance settings
    export CLAUDE_OUTPUT_DIR="${CLAUDE_OUTPUT_DIR:-./generated}"
    export CLAUDE_CACHE_PREFIX="${CLAUDE_CACHE_PREFIX:-cc:}"
    export CLAUDE_REDIS_HOST="${CLAUDE_REDIS_HOST:-localhost}"
    export CLAUDE_REDIS_PORT="${CLAUDE_REDIS_PORT:-6379}"
    export CLAUDE_REDIS_DB="${CLAUDE_REDIS_DB:-0}"
    export CLAUDE_RETRY_COUNT="${CLAUDE_RETRY_COUNT:-3}"
    export CLAUDE_TIMEOUT="${CLAUDE_TIMEOUT:-300}"
    
    echo "‚öôÔ∏è Performance Configuration Applied:"
    echo "  üî¢ Batch Size: $CLAUDE_BATCH_SIZE"
    echo "  ‚ö° Parallel Jobs: $CLAUDE_PARALLEL_JOBS"
    echo "  üíæ Cache TTL: $CLAUDE_CACHE_TTL seconds"
    echo "  üéØ Mode: $CLAUDE_PERFORMANCE_MODE"
    echo ""
}

# üìÅ Smart module discovery and loading! üîç
claude_load_modules() {
    local script_dir="${0:A:h}"  # üìÇ Get script directory
    local modules_loaded=0
    local modules_failed=0
    
    echo "üìö Loading Claude Function Modules..."
    echo "üìÇ Module directory: $script_dir"
    echo ""
    
    # üéØ Priority loading order for optimal performance
    local priority_modules=(
        "claude-async-cache.zsh"      # üíæ Cache first for immediate benefits
        "claude-parallel-cache.zsh"   # üíæ Parallel cache variant
        "claude-async-database.zsh"   # üóÑÔ∏è Database operations
        "claude-parallel-database.zsh" # üóÑÔ∏è Parallel database variant
        "claude-async-files.zsh"      # üìÅ File processing
        "claude-parallel-files.zsh"   # üìÅ Parallel file variant
        "claude-async-git.zsh"        # üêô Git automation
        "claude-parallel-git.zsh"     # üêô Parallel git variant
        "claude-async-docker.zsh"     # üê≥ Docker operations
        "claude-parallel-docker.zsh"  # üê≥ Parallel docker variant
        "claude-async-general.zsh"    # üîß General utilities
        "claude-parallel-general.zsh" # üîß Parallel general variant
    )
    
    # üîÑ Load each module with error handling
    for module in "${priority_modules[@]}"; do
        local module_path="$script_dir/$module"
        
        if [[ -f "$module_path" ]]; then
            echo "  üì¶ Loading: $module"
            if source "$module_path" 2>/dev/null; then
                ((modules_loaded++))
                echo "    ‚úÖ Success!"
            else
                ((modules_failed++))
                echo "    ‚ùå Failed to load!"
            fi
        else
            echo "  ‚ö†Ô∏è Not found: $module (will be created)"
        fi
    done
    
    echo ""
    echo "üìä Module Loading Summary:"
    echo "  ‚úÖ Loaded: $modules_loaded modules"
    echo "  ‚ùå Failed: $modules_failed modules"
    echo "  üéØ Total Available: ${#priority_modules[@]} modules"
    echo ""
    
    # üéâ Success celebration if most modules loaded
    if [[ $modules_loaded -gt 0 ]]; then
        echo "üéâ Claude Function Modules loaded successfully!"
        echo "üí° Type 'claude-perf-status' to see available functions"
    else
        echo "‚ö†Ô∏è No modules loaded. Run setup first!"
        echo "üí° Check module files exist in: $script_dir"
    fi
}

# üîß Auto-detection and system optimization! üéõÔ∏è
claude_performance_autodetect() {
    echo "üîç Auto-detecting system capabilities..."
    
    # üåê Check Redis availability
    if command -v redis-cli >/dev/null 2>&1; then
        if redis-cli ping >/dev/null 2>&1; then
            echo "  ‚úÖ Redis: Available and responsive"
            export CLAUDE_REDIS_AVAILABLE="true"
        else
            echo "  ‚ö†Ô∏è Redis: CLI available but server not responding"
            export CLAUDE_REDIS_AVAILABLE="false"
        fi
    else
        echo "  ‚ùå Redis: Not available (caching disabled)"
        export CLAUDE_REDIS_AVAILABLE="false"
    fi
    
    # üê≥ Check Docker availability
    if command -v docker >/dev/null 2>&1; then
        if docker info >/dev/null 2>&1; then
            echo "  ‚úÖ Docker: Available and running"
            export CLAUDE_DOCKER_AVAILABLE="true"
        else
            echo "  ‚ö†Ô∏è Docker: CLI available but daemon not running"
            export CLAUDE_DOCKER_AVAILABLE="false"
        fi
    else
        echo "  ‚ùå Docker: Not available"
        export CLAUDE_DOCKER_AVAILABLE="false"
    fi
    
    # üêô Check Git availability
    if command -v git >/dev/null 2>&1; then
        echo "  ‚úÖ Git: Available"
        export CLAUDE_GIT_AVAILABLE="true"
        
        # üè∑Ô∏è Check if we're in a git repo
        if git rev-parse --git-dir >/dev/null 2>&1; then
            echo "    üìÇ Current directory is a Git repository"
            export CLAUDE_IN_GIT_REPO="true"
        else
            echo "    üìÇ Not in a Git repository"
            export CLAUDE_IN_GIT_REPO="false"
        fi
    else
        echo "  ‚ùå Git: Not available"
        export CLAUDE_GIT_AVAILABLE="false"
        export CLAUDE_IN_GIT_REPO="false"
    fi
    
    # üîß Check Claude Code CLI availability
    if command -v claude-code >/dev/null 2>&1; then
        echo "  ‚úÖ Claude Code CLI: Available"
        export CLAUDE_CLI_AVAILABLE="true"
    else
        echo "  ‚ùå Claude Code CLI: Not found in PATH"
        export CLAUDE_CLI_AVAILABLE="false"
        echo "    üí° Install from: https://docs.anthropic.com/claude-code"
    fi
    
    # ‚ö° Check zsh-async availability
    if [[ -f ~/.zsh-async/async.zsh ]]; then
        echo "  ‚úÖ zsh-async: Available"
        source ~/.zsh-async/async.zsh
        export CLAUDE_ASYNC_AVAILABLE="true"
    else
        echo "  ‚ùå zsh-async: Not found"
        echo "    üí° Install: git clone https://github.com/mafredri/zsh-async ~/.zsh-async"
        export CLAUDE_ASYNC_AVAILABLE="false"
    fi
    
    echo ""
}

